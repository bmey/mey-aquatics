{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.4.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "cloudinary-react": "^1.0.6",
    "lodash": "^4.17.11",
    "modern-normalize": "^0.5.0",
    "query-string": "^6.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-icons": "^3.7.0",
    "react-media": "^1.10.0",
    "react-router-dom": "^5.0.1",
    "reactstrap": "^8.0.1"
  },
  "scripts": {
    "start-js": "react-app-rewired start",
    "start-server": "json-server mock-server/db.json",
    "start": "npm-run-all -p -l start-js start-server build-css cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config watchForFileChanges=false,video=false",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "yarn build-css --watch",
    "build-js": "react-app-rewired build",
    "build": "npm-run-all build-css build-js",
    "firebase-deploy": "firebase deploy",
    "deploy": "npm-run-all --serial build firebase-deploy",
    "lint:fix": "prettier --write \"{src,cypress}/**/*.{js,jsx,scss,json}\" \"*.json\"",
    "test": "react-scripts test --env=jsdom",
    "test:integration": "start-server-and-test start-js http://localhost:3000 cypress:run",
    "test:all": "cross-env CI=true BROWSER=none npm-run-all -p -l test:integration test",
    "stats": "npm run build-js -- --stats",
    "analyze": "npx webpack-bundle-analyzer ./build/bundle-stats.json",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "cypress": "^3.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "firebase-tools": "^7.3.2",
    "json-server": "^0.15.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prettier": "1.18.2",
    "prettier-stylelint": "^0.4.2",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "3.1.2",
    "react-test-renderer": "^16.9.0",
    "start-server-and-test": "^1.10.2",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "webpack-bundle-analyzer": "^3.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}